%table{:border => 1, class: 'table table-striped'}
  %tr
    %th Name
    %th Up By
    %th Percentage Change
    %th New Check In
  - @people.each do |person|
    %tr
      %td #{person.name}
      %td #{person.up_by}
      %td #{person.percentage_change}
      %td= link_to('Check In', new_checkin_path(person_id: person))

%br
= link_to('Create a new person', new_person_path)
